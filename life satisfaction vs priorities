    "## PROBLEM 2\n",
    "\n",
    "import matplotlib.pyplot as plt\n",
    "import pandas as pd\n",
    "import numpy as np\n",
    "import seaborn as sns          \n",
    "\n",
    "%matplotlib notebook\n",
    "%matplotlib inline\n",
    "\n",
    "url = 'https://raw.githubusercontent.com/jbebop/csv/main/oecdbetterlifeindex.csv'\n",
    "df = pd.read_csv(url, header = 0,\n",
    "                usecols = ['life_satisfaction', 'top_issue', 'country'])     \n",
    "df.head()\n",
    "count = df.groupby(['top_issue','life_satisfaction'])['country'].count() \n",
    "count\n",
    "count = count.reset_index()\n",
    "count\n",
    "LS = ['low', 'avg', 'high']        \n",
    "top_issue = count[count.life_satisfaction.isin(LS)]    \n",
    "top_issue\n",
    "\n",
    "#grouped barplots\n",
    "grouped = sns.catplot(kind='bar', x='top_issue', y=\"country\", hue=\"life_satisfaction\", data=top_issue, color='green', size = 5, aspect = 2)   # save to `grouped`\n",
    "grouped.set_axis_labels(\"What Matters Most\", \"Number of Countries\")                           # add labels\n",
    "grouped.fig.suptitle('Life Satisfaction and What Matters Most to A Country')     # add title"
   ]
  },
