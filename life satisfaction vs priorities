    "##PROBLEM 1\n",
    "\n",
    "import matplotlib.pyplot as plt\n",
    "import pandas as pd\n",
    "import numpy as np\n",
    "import seaborn as sns          \n",
    "\n",
    "%matplotlib notebook\n",
    "%matplotlib inline\n",
    "\n",
    "\n",
    "# prepare and read in data\n",
    "url = 'https://raw.githubusercontent.com/jbebop/csv/main/oecdbetterlifeindex.csv'\n",
    "df = pd.read_csv(url, header = 0,\n",
    "                usecols = ['life_satisfaction', 'top_issue', 'country'])     \n",
    "df.head()\n",
    "count = df.groupby(['top_issue','life_satisfaction'])['country'].count() \n",
    "count\n",
    "count = count.reset_index()\n",
    "count\n",
    "#bool slicing\n",
    "LS = ['low', 'avg', 'high']        \n",
    "top_issue = count[count.life_satisfaction.isin(LS)]    \n",
    "top_issue\n",
    "\n",
    "\n",
    "## PROBLEM 1\n",
    "#side by side bar plots\n",
    "count = sns.FacetGrid(top_issue, col = 'life_satisfaction', col_wrap = 3, sharey=False, sharex=False, height = 4, aspect=1.4)    # initialize a facet grid object and save to a variable\n",
    "count.map(sns.barplot, 'top_issue', 'country') \n",
    "\n",
    "## grid customizations\n",
    "count.set_axis_labels('', '')                                             \n",
    "count.set_titles(col_template='{col_name}')                               \n",
    "count.fig.text(0.5, 0.02, 'What Matters Most', ha='center', va='center')     \n",
    "count.fig.text(0.01, 0.5, 'Number of Countries from Data)', ha='center', va='center', rotation='vertical')      \n",
    "count.fig.suptitle('Life Satisfaction and What Matters Most to a Country', y = 1.2)      "
   ]
